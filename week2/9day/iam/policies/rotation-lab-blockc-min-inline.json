{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowManageOwnAccessKeysOnly",
      "Effect": "Allow",
      "Action": [
        "iam:ListAccessKeys",
        "iam:CreateAccessKey",
        "iam:UpdateAccessKey",
        "iam:DeleteAccessKey",
        "iam:GetAccessKeyLastUsed"
      ],
      "Resource": "arn:aws:iam::220197469675:user/${aws:username}"
    },
    {
      "Sid": "AllowPutSecretValueToRotationSecretOnly",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:DescribeSecret",
        "secretsmanager:PutSecretValue"
      ],
      "Resource": "arn:aws:secretsmanager:*:220197469675:secret:lab/iam/rotation-lab-user-??????"
    }
  ]
}
