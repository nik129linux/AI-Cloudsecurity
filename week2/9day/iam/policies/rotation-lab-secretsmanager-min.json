{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCreateOnlyThisSecretName",
      "Effect": "Allow",
      "Action": "secretsmanager:CreateSecret",
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "secretsmanager:Name": "lab/iam/rotation-lab-user"
        }
      }
    },
    {
      "Sid": "AllowTagDescribePutOnlyThisSecret",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:TagResource",
        "secretsmanager:DescribeSecret",
        "secretsmanager:PutSecretValue"
      ],
      "Resource": "arn:aws:secretsmanager:us-east-1:220197469675:secret:lab/iam/rotation-lab-user-??????"
    }
  ]
}